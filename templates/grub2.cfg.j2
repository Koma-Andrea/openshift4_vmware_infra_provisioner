set pager=1
search --label boot --set boot
set root=$boot
if [ -f ${config_directory}/grubenv ]; then
  load_env -f ${config_directory}/grubenv
elif [ -s $prefix/grubenv ]; then
  load_env
fi
if [ x"${feature_menuentry_id}" = xy ]; then
  menuentry_id_option="--id"
else
  menuentry_id_option=""
fi

function load_video {
  if [ x$feature_all_video_module = xy ]; then
    insmod all_video
  else
    insmod efi_gop
    insmod efi_uga
    insmod ieee1275_fb
    insmod vbe
    insmod vga
    insmod video_bochs
    insmod video_cirrus
  fi
}

if [ x$feature_timeout_style = xy ] ; then
  set timeout_style=menu
  set timeout=5
else
  set timeout=5
fi

set ignition_firstboot="CUSTOM_CONFIG_IN_EFFECT"
if [ -f "/ignition.firstboot" ]; then
    set ignition_network_kcmdline='CUSTOM_CONFIG_IN_EFFECT rd.neednet=1 ip={{ item.network.ip }}::{{ item.network.gw |  default(hosts_defaults['network']['gw']) }}:{{ item.network.mask |  default(hosts_defaults['network']['mask']) }}:{{ item.hostname }}:{{ item.network.interface |  default(hosts_defaults['network']['interface']) | default('ens192') }}:none {%- for dns in item.network.dns | default(hosts_defaults.network.dns)  %} nameserver={{dns}} {%- endfor %}' 
    source "/ignition.firstboot"
    set ignition_firstboot="ignition.firstboot ${ignition_network_kcmdline}"
fi

blscfg